ardContainerLayout = {\n    display: 'flex',\n    justifyContent: 'center',\n    flexFlow: 'row wrap'\n}\n\nconst IndexExpenses = (props) => {\n    const [expenses, setExpenses] = useState(null)\n\n    const {user, msgAlert} = props\n\n    useEffect(() => {\n        getAllExpenses()\n            .then(res => {\n                setExpenses(res.data.expenses)\n            })\n            .then(() => {\n                msgAlert({\n                    heading: 'Found some expenses!',\n                    message: indexExpensesSuccess,\n                    variant: 'success',\n                })\n            })\n            .catch(() => {\n                msgAlert({\n                    heading: 'No expenses?!!',\n                    message: indexExpensesFailure,\n                    variant: 'danger',\n                })\n            })\n    }, [])\n\n    if (!expenses) {\n        return <p>loading...</p>\n    } else if (expenses.length === 0) {\n        return <p>no expenses yet, go add some</p>\n    }\n\n    let expenseCards\n\n    if (expenses.length > 0) {\n        expenseCards = vs.map(expense => (\n            // one method of styling, usually reserved for a single style\n            // we can use inline, just like in html\n            <Card key={expense.id} style={{ width: '30%' }} className=\"m-2\">\n                <Card.Header>{expense.fullTitle}</Card.Header>\n                <Card.Body>\n                    <Card.Text>\n                        <Link to={`/expenses/${expense.id}`}>View {expense.name}</Link>\n                    </Card.Text>\n                </Card.Body>\n            </Card>\n        ))\n    }\n\n    return (\n        <>\n            <h3>All the expenses</h3>\n            <div style={cardContainerLayout}>\n                {expenseCards}\n            </div>\n        </>\n    )\n}\n\nexport default IndexExpenses","/Users/andrewoehmann/sei/projects/bank_app/USBA_client/src/components/expenses/CreateExpense.js",["91","92","93"],"import React, { u